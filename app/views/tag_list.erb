<h2>
  タグ一覧
</h2>

<table id="taglist_table" class="table sortable">
  <thead>
    <tr>
      <th>タグ名</th>
      <th>登録曲数</th>
    </tr>
  </thead>
  <tbody>
    <% @tags.each do |tag| %>
    <tr>
      <td><a href="/search/tag/?tag=<%= h tag['name'] %>"><%= h tag['name'] %></a></td>
      <td><%= tag['count'] %></td>
    </tr>
    <% end %>
  </tbody>
</table>
